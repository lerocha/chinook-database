            "System.Int32" => "INT",
            "System.Decimal" => "NUMERIC(10,2)",
            "System.DateTime" => "TIMESTAMP",
            _ => "error_" + column.DataType
        };

        private static string ToSnakeCase(string text) => snakeCaseNamingStrategy.GetPropertyName(text, false);
    }

