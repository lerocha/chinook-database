	public class Db2Strategy : AbstractDdlStrategy
	{
		public Db2Strategy()
		{
			Name = "Db2";
			//Identity = "AUTO_INCREMENT";
			IsReCreateDatabaseEnabled = true;

			var builder = new StringBuilder();
			builder.AppendLine("db2 disconnect ALL")
